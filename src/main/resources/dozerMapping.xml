<?xml version="1.0" encoding="UTF-8" ?>
<mappings xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns="http://dozer.sourceforge.net"
    xsi:schemaLocation="http://dozer.sourceforge.net
          http://dozer.sourceforge.net/schema/beanmapping.xsd">
    <mapping map-null="false">
        <class-a>com.epam.test_generator.entities.Step</class-a>
        <class-b>com.epam.test_generator.dto.StepDTO</class-b>
        <field>
            <a get-method="getType" set-method="setType">type</a>
            <b get-method="getType" set-method="setType">type</b>
        </field>
    </mapping>
    <mapping map-null="false">
        <class-a>com.epam.test_generator.entities.Suit</class-a>
        <class-b>com.epam.test_generator.dto.SuitDTO</class-b>
    </mapping>
    <mapping map-null="false">
        <class-a>com.epam.test_generator.entities.Tag</class-a>
        <class-b>com.epam.test_generator.dto.TagDTO</class-b>
    </mapping>
    <mapping map-null="false">
        <class-a>com.epam.test_generator.entities.StepSuggestion</class-a>
        <class-b>com.epam.test_generator.dto.StepSuggestionDTO</class-b>
        <field>
            <a get-method="getType" set-method="setType">type</a>
            <b get-method="getType" set-method="setType">type</b>
        </field>
    </mapping>
    <mapping map-null="false">
        <class-a>com.epam.test_generator.entities.User</class-a>
        <class-b>com.epam.test_generator.dto.UserDTO</class-b>
        <field>
            <a>role.name</a>
            <b>role</b>
        </field>
    </mapping>
    <mapping map-null="false">
        <class-a>com.epam.test_generator.pojo.CaseVersion</class-a>
        <class-b>com.epam.test_generator.dto.CaseVersionDTO</class-b>
    </mapping>
    <mapping map-null="false">
        <class-a>com.epam.test_generator.pojo.PropertyDifference</class-a>
        <class-b>com.epam.test_generator.dto.PropertyDifferenceDTO</class-b>
        <field custom-converter="com.epam.test_generator.transformers.CustomCaseVersionConverter">
            <a>oldValue</a>
            <b>oldValue</b>
        </field>
        <field custom-converter="com.epam.test_generator.transformers.CustomCaseVersionConverter">
            <a>newValue</a>
            <b>newValue</b>
        </field>
    </mapping>
</mappings>